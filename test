int x, y, z;

int boo(int input[]) {
	-- Some sort of compound statement
	{
		int t;
		t := 42;
		; -- Check out this null statement. AH YEAH
	}

	input[1] := readint();
	if(input[1] = 4) {
		writeint(input[1]);
		return 3;
	}

	branch (input[0])
		case 4:
			return input[0] * 2;
		default:
			return input[0] * 500;
		/*case 4:
			return input[0] - 4;*/
	end;

	return 5;
}

/*int readint(void) {
	return 6;
}*/

void foo(void) {
	return;
}

int main(void) {
	int x[4], y;
	int bad[1*3];
	bool yourFace;
	bool youSuck;
	int meaning_of_life;

	x[0] := 0+1;
	x[1] := boo(x);
	/*y := foo();
	boo(x);*/
	if(y < 3) {
		;
	}

	if(x[y-1]=0){
		yourFace := not true;
		youSuck := true;
	}
	else{
		x:=-5;
	}

	-- Dangling else
	if(yourFace)
		if(x[0] = 1)
			x[0] := 2;
		else
			x[0] := 1;
	if(youSuck)
		if(x[0] = 1)
			x[0] := 2;
		else
			x[0] := 1;
	else
		youSuck := true;

	loop x:=x*(1+2);
	if(x>=10)
		exit;
	if(y = 4) {
		continue;
	}
	end;

	loop x:=x+1;
	loop y:=y+1;
		exit;
	end;
	if(y = 7) {
		int xyz;
		loop x:=x-1;
			exit;
		end;
	}
	continue;
	end;

	meaning_of_life := 42;

	return 3;
}
