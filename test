int x, y, z;

int boo(int input[]) {
	-- Some sort of compound statement
	{
		int t;
		t := 42;
		; -- Check out this null statement. AH YEAH
	}

	branch (input[0])
		case 4:
			return input[0] * 2;
		default:
			return input[0] * 500;
	end;
}

void foo(void) {
	return 5;
}

int main(void) {
	int x[4], y;
	bool yourFace;
	bool youSuck;
	int meaning_of_life;

	x[0] := 0+1;
	x[1] := boo(x);
	foo();
	if(y < 3) {
		--main(param, not t);
		;
	}

	if(x[y-1]=0){
		yourFace := not true;
		youSuck := true;
	}
	else{
		x:=-5;
	}

	-- Dangling else
	if(yourFace)
		if(x[0] = 1)
			x[0] := 2;
		else
			x[0] := 1;
	if(youSuck)
		if(x[0] = 1)
			x[0] := 2;
		else
			x[0] := 1;
	else
		youSuck := true;

	loop x:=x*(1+2);
	if(x>=10)
		exit;
	if(y = 4) {
		continue;
	}
	end;

	loop x:=x+1;
	loop y:=y+1;
		exit;
	end;
	if(y = 7) {
		exit;
	}
	continue;
	end;

	meaning_of_life := 42;
}
