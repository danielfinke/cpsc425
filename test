int x, y, z;

int boo(int input[]) {
	--return input + 3;
	input := 3;
	{
		int t;
		t := 42;
	}
}

int foo(void) {
	/*return 5;*/
	input := 3;
}

void main(ref int param) {
	int x[4], y;
	bool yourFace;
	bool youSuck;
	int meaning_of_life;

	x[0] := 0+1;
	x[1] := boo(x);
	y := foo();

	if(x[y-1]=0){
		yourFace := false;
		youSuck := true;
	}
	else{
		x:=-5;
	}

	loop x:=(1*2)+x;
	if(x>=10)
		exit;
	end;

	meaning_of_life := 42;
}
