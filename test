int x, y, z;

int boo(int input[]) {
	-- Some sort of compound statement
	{
		int t;
		t := 42;
		; -- Check out this null statement. AH YEAH
	}

	branch (input[0])
		case 4:
			return input[0] * 2;
		default:
			return input[0] * 500;
	end;
}

int foo(void) {
	return 5;
}

void main(ref int param) {
	int x[4], y;
	bool yourFace;
	bool youSuck;
	int meaning_of_life;

	x[0] := 0+1;
	x[1] := boo(x);
	y := foo();

	if(x[y-1]=0){
		yourFace := false;
		youSuck := true;
	}
	else{
		x:=-5;
	}

	loop x:=(1*2)+x;
	if(x>=10)
		exit;
	if(y = 4) {
		continue;
	}
	end;

	meaning_of_life := 42;
}
